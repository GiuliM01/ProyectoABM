@model Facturacion
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<table>
    <thead>
        <tr>
            <th>Cliente</th>
            <th>Numero</th>
            <th>Precio</th>
            <th>Codigo</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var facturacion in ViewBag.Lista)
        {            
        <tr>
            <td>@facturacion.Cliente</td>
            <td>@facturacion.Numero</td>
            <td>@facturacion.Precio</td>
            <td>@facturacion.Codigo</td>
            <td>
            </td>
        </tr>
        }
    </tbody>
</table>
